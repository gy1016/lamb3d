{
  "name": "lamb3d",
  "version": "1.1.0",
  "description": "A 3d gis engine written in TypeScript and WebAssembly.",
  "author": "lamb",
  "license": "MIT",
  "keywords": [
    "gis",
    "webgl",
    "wasm"
  ],
  "homepage": "https://github.com/gy1016/lamb3d",
  "repository": {
    "type": "git",
    "url": "https://github.com/gy1016/lamb3d"
